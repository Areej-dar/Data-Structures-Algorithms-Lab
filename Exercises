#1-To check palindrome string
string=input("Enter a string: ")
reverse=string[::-1]
stack=[]

for letters in string:
    stack.append(letters)
print(stack)
    
if (reverse==string):
    print("The entered string is a palindrome")
else:
    print("The entered string is not a palindrome")

#2-To check the balanced expression
exp=input ("Enter the braces: ")
stack1=[]

for ch in exp:
    if (ch=="[" or ch=="(" or ch=="{"):
        stack1.append(ch)
    elif(ch=="]" or ch==")" or ch=="}"):
        stack1.pop()
        
if(len(stack1)==0):
    print ("Valid Expression")
else:
    print ("Invalid Expression")
